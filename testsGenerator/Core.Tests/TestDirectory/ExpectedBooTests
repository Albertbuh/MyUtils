using Moq;
using Core.Tests.Entities;

namespace Core.Tests.Entities.Tests;
[TestClass]
public class Boo_Tests
{
    private Boo Boo;
    private Mock<IDependency> _dependency;
    [TestInitialize]
    public void Initialize()
    {
        _dependency = new Mock<IDependency>();
        Boo = new Boo(_dependency.Object);
    }

    [TestMethod]
    public void SumOfTwo()
    {
        int a = 0;
        int b = 0;
        int actual = Boo.SumOfTwo(a, b);
        int expected = 0;
        Assert.Equal(actual, expected);
        Assert.Fail(autogenerated);
    }

    [TestMethod]
    public void SomeString()
    {
        Foo foo = null;
        string actual = Boo.SomeString(foo);
        string expected = "";
        Assert.Equal(actual, expected);
        Assert.Fail(autogenerated);
    }
}
